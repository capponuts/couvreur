{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/(.*\\.(jpg|jpeg|png|gif|svg|mp4|webm|ico|txt))",
      "dest": "/$1"
    }
  ]
}
